{
  "name": "atavism-editor",
  "version": "0.0.6",
  "main": "main.js",
  "author": {
    "name": "Vasyl Korol",
    "email": "vasyl.korol@gmail.com"
  },
  "scripts": {
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "yarn postinstall:electron && electron-builder install-app-deps",
    "postinstall:web": "node postinstall-web",
    "postinstall:electron": "node postinstall",
    "ng": "ng",
    "start": "yarn postinstall:electron && npm-run-all -p ng:serve electron:serve",
    "build": "yarn postinstall:electron && yarn electron:serve-tsc && ng build",
    "build:dev": "yarn build -- -c dev",
    "build:prod": "yarn build -- -c production",
    "ng:serve": "ng serve -o",
    "ng:serve:web": "yarn postinstall:web && ng serve",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "electron:serve": "wait-on http-get://localhost:4200/ && yarn electron:serve-tsc && electron . --serve",
    "electron:local": "yarn build:prod && electron .",
    "electron:linux": "yarn build:prod && electron-builder build --linux",
    "electron:windows": "yarn build:prod && electron-builder build --windows",
    "electron:mac": "yarn build:prod && electron-builder build --mac",
    "electron": "ng build --base-href ./ && electron .",
    "release:repo": "git push --follow-tags origin master",
    "release:patch": "standard-version -- --release-as path && yarn release:repo",
    "release:minor": "standard-version -- --release-as minor && yarn release:repo",
    "release:major": "standard-version -- --release-as major && yarn release:repo"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@agm/core": "^1.0.0",
